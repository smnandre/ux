<div style="display:grid; grid-template-columns: repeat(auto-fill, minmax(340px, 1fr)); gap: 2rem;">

    {% for iconSet in this.getIconSets() %}
        <twig:Icon:IconSetCard
            iconSet="{{ iconSet }}"
            index="{{ loop.index }}"
            samples="{{ this.samples }}"
        />
    {% endfor %}

</div>
